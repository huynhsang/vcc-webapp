{
  "name": "vcc-webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.18.1",
    "dotenv": "8.0.0",
    "font-awesome": "4.7.0",
    "jquery": "3.4.1",
    "node-sass": "4.12.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "react-slick": "0.24.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "slick-carousel": "1.8.1",
    "sweetalert-react": "0.4.11"
  },
  "scripts": {
    "start": "sh -ac '. ./.env.local; PORT=8080 react-scripts start'",
    "start:prod": "sh -ac '. ./.env.prod; PORT=8080 react-scripts start'",
    "build": "sh -ac '. ./.env.local; react-scripts build'",
    "build:prod": "sh -ac '. ./.env.prod; react-scripts build'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
